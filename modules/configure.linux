#!/bin/bash
#
# Linux support
# Sets:
#

function checks {

    mkl_meta_set zookeeper deb "libzookeeper-dev"

    if ! mkl_pkg_config_check zookeeper HAVE_ZOOKEEPER cont; then
        mkl_compile_check zookeeper HAVE_ZOOKEEPER disable CC "-lzookeeper" \
            "
#include <zookeeper.h>
void test {
  zookeeper_init();
}
"
    fi

}